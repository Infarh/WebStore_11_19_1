@model IEnumerable<WebStore.ViewModels.EmployeeView>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetEmployes</title>
</head>
<body>

    <h3>@ViewBag.SomeData</h3>
    <h4>@ViewData["Test"]</h4>

    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>Фамилия 123</th>
                <th>Имя</th>
                <th>Отчество 321</th>
                <th>Возраст !!!</th>
                <th>id</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.SecondName</td>
                    <td>@employee.FirstName</td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                    <td>@employee.Id</td>
                </tr>
            }
        </tbody>
    </table>

    <hr />

    <ul>
        @foreach (var employee in Model)
        {
            <li>@employee.SecondName</li>
        }
    </ul>
</body>
</html>
