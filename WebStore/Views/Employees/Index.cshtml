@model IEnumerable<WebStore.ViewModels.EmployeeView>

@{
    ViewBag.Title = "Сотрудники";
}

<h3>@ViewBag.Title</h3>

<table class="table">
    <thead>
    <tr>
        <th>id</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Возраст</th>
    </tr>
    </thead>

    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.SecondName</td>
            <td>@employee.FirstName</td>
            <td>@employee.Patronymic</td>
            <td>@employee.Age</td>
        </tr>
    }
    </tbody>
</table>